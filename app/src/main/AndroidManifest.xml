<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dione.attendanceapp">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
    android:name="com.example.dione.attendanceapp.application.AttendanceApplication"
        android:allowBackup="true"
        android:icon="@drawable/app_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >

        <meta-data android:name="DATABASE" android:value="attendance.db" />
        <meta-data android:name="VERSION" android:value="2" />
        <meta-data android:name="QUERY_LOG" android:value="true" />
        <meta-data android:name="DOMAIN_PACKAGE_NAME" android:value="com.example.dione.attendanceapp" />


        <activity android:name="com.example.dione.attendanceapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.dione.attendanceapp.LogsActivity">
        </activity>
    </application>

</manifest>